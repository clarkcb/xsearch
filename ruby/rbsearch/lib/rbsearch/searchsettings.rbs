    attr_accessor out_lines_before_patterns: Set[Regexp]
    attr_accessor print_lines: bool
    attr_accessor print_results: bool
    attr_accessor search_archives: bool
    attr_accessor search_patterns: Set[Regexp]
    attr_accessor text_file_encoding: String
    attr_accessor unique_lines: bool

