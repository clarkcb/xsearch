{
  "path": ["~/src/xsearch/javascript/", "~/src/xsearch/typescript/"],
  "in-ext": ["js", "ts"],
  "out-dirpattern": [
    "_", "ansible", "bak", "build", "chef", "dist", "node_module", "target", "test", "typings", "vendor", "venv"
  ],
  "out-filepattern": ["gulpfile", "\\.min\\."],
  "searchpattern": "Searcher",
  "linesbefore": 2,
  "linesafter": 2,
  "colorize": false,
  "debug": false,
  "allmatches": false,
  "includehidden": false,
  "maxdepth": 10,
  "maxsize": 10000,
  "printdirs": true,
  "printfiles": true
}
